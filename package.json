{
  "name": "ai-backend-service",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "dev:frontend": "cd frontend && npm start",
    "dev:backend": "nodemon index.js",
    "dev:all": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build": "cd frontend && npm run build",
    "install:all": "npm install && cd frontend && npm install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration"
  },
  "keywords": [
    "api",
    "express",
    "nodejs",
    "backend",
    "stocks",
    "finance",
    "alpha-vantage",
    "react",
    "frontend"
  ],
  "author": "",
  "license": "ISC",
  "description": "A stock data API service with Alpha Vantage integration and React frontend with candlestick charts",
  "dependencies": {
    "@polygon.io/client-js": "^7.4.0",
    "alphavantage": "^2.5.0",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "helmet": "^7.1.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "yahoo-finance2": "^2.13.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.10",
    "@types/jest": "^30.0.0",
    "concurrently": "^8.2.2",
    "jest": "^30.0.2",
    "nock": "^14.0.5",
    "nodemon": "^3.0.2",
    "supertest": "^7.1.1"
  }
}
